{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "app": {
    "PacsStorageConnection": "UseDevelopmentStorage=true;",
    //"PacsStorageConnection": "|DataDirectory|\\DICOMcloud.Wado.WebApi.Core\\App_Data\\Storage\\ds",
    "PacsDataArchieve": "Data Source=(LocalDb)\\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\\DB\\DICOMcloud.mdf;Initial Catalog=DICOMcloud;Integrated Security=True",
    "WadoRsUrl": "https://localhost:44301/",
    "WadoUriUrl": "http://localhost:44301/wadouri",
    "enableAnonymizer": true,
    "anonymizerOptions": "BasicProfile,RetainUIDs,RetainLongFullDates,RetainPatientChars",
    "supportPreSignedUrls": true
  },
  "enableDeIdentifier": false,
  "deIdentifierOptions": "BasicProfile,RetainUIDs,RetainLongFullDates,RetainPatientChars",
  "qido": {
    "maximumResultsLimit": 100
  },
  "storecommand": {
    "validateDuplicateInstance": false,
    "storeOriginalDataset": true,
    "storeQueryModel": true,
    "mediaTypes": [
      {
        "mediaType": "application/dicom",
        "transferSyntax": "1.2.840.10008.1.2.1"
      },
      {
        "mediaType": "application/dicom+json"
      },
      {
        "mediaType": "application/octet-stream",
        "transferSyntax": "1.2.840.10008.1.2.1"
      },
      {
        "mediaType": "application/dicom+xml"
      },
      {
        "mediaType": "image/jpeg"
      }
    ]
  },
  "cors": {
    "enabled": true,
    "origins": "*",
    "headers": "*",
    "methods": "*"
  }
}