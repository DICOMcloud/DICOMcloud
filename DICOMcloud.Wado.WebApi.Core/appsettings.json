{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",  
  "app": {
    "PacsStorageConnection": "|DataDirectory|\\App_Data\\Storage\\ds",
    "PacsDataArchieve": "Data Source=(LocalDb)\\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\\DB\\DICOMcloud.mdf;Initial Catalog=DICOMcloud;Integrated Security=True",
    "WadoRsUrl": "https://localhost:44301/",
    "WadoUriUrl": "http://localhost:44301/wadouri",
    "enableAnonymizer": true,
    "anonymizerOptions": "BasicProfile,RetainUIDs,RetainLongFullDates,RetainPatientChars",
    "supportPreSignedUrls": true
  },
  "enableDeIdentifier": false,
  "deIdentifierOptions": "BasicProfile,RetainUIDs,RetainLongFullDates,RetainPatientChars",
  "storecommand": {
    "validateDuplicateInstance": "",
    "storeOriginalDataset": "",
    "storeQueryModel": ""
  },
  "cors": {
    "enabled": true,
    "origins": "*",
    "headers": "*",
    "methods": "*"
  }
}
